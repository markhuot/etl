{
    "name": "markhuot/etl",
    "description": "A simple ETL library for PHP",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "markhuot",
            "email": "mark@markhuot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "markhuot\\etl\\": "./src"
        },
        "files": [
            "src/helpers/exceptions.php"
        ]
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "symfony/var-dumper": "^7.2",
        "laravel/pint": "^1.18",
        "phpstan/phpstan": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
